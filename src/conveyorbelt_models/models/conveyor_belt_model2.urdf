        rpy="0 0 0" />
      <mass
        value="0.3" />
      <inertia
        ixx="5.0271E-05"
        ixy="-1.0048E-20"
        ixz="-2.5529E-22"
        iyy="0.00035694"
        iyz="3.288E-21"
