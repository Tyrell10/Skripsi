    name="convey2_motor_joint"
    type="continuous">
    <origin
      xyz="0.069565 -0.15776 -0.020936"
      rpy="0 0 0" />
    <parent
      link="base_link_convey2" />
    <child
      link="convey2_motor_link" />
    <axis
      xyz="1 0 0" />
    <limit
      effort="100"
      velocity="2.2" />
    <joint_properties damping="0.0"/>
  </joint>
  <link
